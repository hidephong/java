# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET        /                    controllers.Application.index
GET        /ans.json            controllers.Application.ansJson
GET        /quiz                controllers.Application.quiz
GET        /getJson             controllers.Application.getJson
GET        /getQuest            controllers.Application.getQuest
GET        /getAns              controllers.Application.getAns


# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file        controllers.Assets.at(path="/public", file)

    