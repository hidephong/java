    private void Row2Info() {
        if (jtbPost.getSelectedRow() >= 0) {
            int index = jtbPost.convertRowIndexToModel(jtbPost.getSelectedRow());
            jtfID.setText("" + pVector.get(index).getRoleID());
            jtfRole.setText(pVector.get(index).getRole());
            jftPostAllowance.setText("" + pVector.get(index).getPostAllowance());
            jcbStatus.setSelected(pVector.get(index).getBStatus());
            jbtSave.setVisible(false);
            jbtCancel.setVisible(false);
        }
    }

   private void initData(){
        pData = new PostData();
        pVector = pData.getAllPost();
        pModel = new PostModel(pVector);
        jtbPost.setModel(pModel);
        jtbPost.revalidate();
        emptyFields();
    }

