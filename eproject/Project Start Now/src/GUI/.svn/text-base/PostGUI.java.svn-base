    private void jbtEditActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jbtEditActionPerformed
        if(jtbPost.getSelectedRow() >= 0){
            jtfRole.setEditable(true);
            jcbStatus.setEnabled(true);
            jbtCancel.setVisible(true);
            jbtSave.setVisible(true);
        }else{
            JOptionPane.showMessageDialog(this, "You must choose a Post to edit!");
            return;
        }
}//GEN-LAST:event_jbtEditActionPerformed

    private void jbtDelActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jbtDelActionPerformed
        int iRow  = jtbPost.convertRowIndexToModel(jtbPost.getSelectedRow()) ;
        if (iRow >= 0) {
            String Confrm = "Do you want delete Post as: " + pVector.get(iRow).getRole() + " ?";
            if(JOptionPane.showConfirmDialog(this, Confrm, "Confirm Delete?", JOptionPane.YES_NO_OPTION)==0){
                int delID = Integer.valueOf(jtfID.getText());
                try {
                    boolean isDeleted = pData.deleteAPost(delID);
                    if(isDeleted){
                      initData();
                    }
                } catch (Exception e) {
                }
            }
        }
}//GEN-LAST:event_jbtDelActionPerformed

    private void jbtCancelActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jbtCancelActionPerformed
            Row2Info();

}//GEN-LAST:event_jbtCancelActionPerformed

    private void jbtSaveActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jbtSaveActionPerformed
        if(jtfRole.getText().isEmpty() || jftPostAllowance.getText().isEmpty()){
            System.out.println(jftPostAllowance.getText());
            JOptionPane.showMessageDialog(this, "The Department Name and Loacation can not empty!","Warning",JOptionPane.OK_OPTION);
            return;
        }else{
           Post p = new Post();
           p.setRoleID(Integer.parseInt(jtfID.getText()));
           p.setRole(jtfRole.getText());
           p.setPostAllowance(Float.parseFloat(jftPostAllowance.getText()));
           p.setBStatus(jcbStatus.isSelected());

           boolean update = pData.updateAPost(p);
           if(update){
                JOptionPane.showMessageDialog(this, "Update Sucssesfull!");
                initData();
            }else{
                JOptionPane.showMessageDialog(this, "Update false");
            }
        }
}//GEN-LAST:event_jbtSaveActionPerformed

    private void jbtSearchActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jbtSearchActionPerformed
      String role2s = jtfRolekywrd.getText();
      String stt2s = jcbbStatus.getSelectedItem().toString();
      if(stt2s.equals("All")){
          stt2s = "";
      }
      Vector<Post> searchPost = pData.searchPost(role2s, stt2s);
      pModel = new PostModel(searchPost);
      jtbPost.setModel(pModel);
      jtbPost.revalidate();

    }//GEN-LAST:event_jbtSearchActionPerformed

