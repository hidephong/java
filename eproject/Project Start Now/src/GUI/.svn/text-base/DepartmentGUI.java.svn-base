    private void afterInit(){
        departmentVector = deptdata.getAllDepartment();
        locationVector = locdata.getAllLocation();
        jcbbLocationModel = new String[locationVector.size()];
        int i =0;
        for (Location loc : locationVector) {
             jcbbLocationModel[i] = loc.getLocationAddress();
             System.out.println(loc.getLocationAddress());
            i++;
        }
        setCbbmdl(new DefaultComboBoxModel(jcbbLocationModel));
        jcbbLocation.setModel(getCbbmdl());
        
        departmentModel = new DepartmentModel(departmentVector);
        jtbDepartment.setModel(departmentModel);
        jtbDepartment.revalidate();
        DisableDeparmentInfo();
        jtfID.setText(null);
        jtfName.setText(null);
        jcbbLocation.setSelectedIndex(-1);
    }

    private void DisableDeparmentInfo() {
        jtfID.setEditable(false);
        jtfName.setEditable(false);
        jcbStatus.setEnabled(false);
        jcbbLocation.setEnabled(false);
        jbtCancel.setVisible(false);
        jbtSave.setVisible(false);
     
    }
