    private void jtbDepartmentMouseClicked(java.awt.event.MouseEvent evt) {//GEN-FIRST:event_jtbDepartmentMouseClicked
        if(jtbDepartment.getSelectedRow() >= 0){
          int index = jtbDepartment.convertRowIndexToModel(jtbDepartment.getSelectedRow());
          jtfID.setText(""+ departmentVector.get(index).getDepartmentID());
          jtfName.setText(departmentVector.get(index).getDepartmentName());
          jcbbLocation.setSelectedItem(departmentVector.get(index).getLocationAddress());
          jcbStatus.setSelected(departmentVector.get(index).getBooleanStatus());        
        }
        
    }//GEN-LAST:event_jtbDepartmentMouseClicked

