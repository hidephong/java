    private void jbtEditActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jbtEditActionPerformed
    if(jtbDepartment.getSelectedRow() >= 0){
        jtfName.setEditable(true);
        jcbStatus.setEnabled(true);
        jcbbLocation.setEnabled(true);
        jbtCancel.setVisible(true);
        jbtSave.setVisible(true);
    }else{
        JOptionPane.showMessageDialog(this, "You must choose a Department to edit!");
        return;
    }
}//GEN-LAST:event_jbtEditActionPerformed
  
    private void jbtDelActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jbtDelActionPerformed

        if(!jtfID.getText().equals(""))
        {
            int iRow  = jtbDepartment.convertRowIndexToModel(jtbDepartment.getSelectedRow()) ;
                if (iRow >= 0) {
                String Confrm = "Do you want delete selected department: " + departmentVector.get(iRow).getDepartmentName() + " ?";
                if(JOptionPane.showConfirmDialog(this, Confrm, "Confirm Delete?", JOptionPane.YES_NO_OPTION)==0){
                    int delID = Integer.valueOf(jtfID.getText());
                    try {
                        boolean isDeleted = deptdata.deleteADepartment(delID);
                        if(isDeleted){
                            RowSorter<? extends TableModel> rowSorter = jtbDepartment.getRowSorter();
                            rowSorter.rowsDeleted(iRow, iRow);
                            jtbDepartment.setModel(rowSorter.getModel());
                            jtbDepartment.revalidate();
                        }
                        else
                        {
                            deptdata.UpdateAfterDelete_Department(delID);
                            RowSorter<? extends TableModel> rowSorter = jtbDepartment.getRowSorter();
                            
                            afterInit();
                            jtbDepartment.setModel(rowSorter.getModel());
                            jtbDepartment.revalidate();
                        }
                    } catch (Exception e) {
                    }
                }
            }
        }
        else
        {
            JOptionPane.showMessageDialog(this, "You must choose a department", "WARNING", JOptionPane.ERROR_MESSAGE);
        }
}//GEN-LAST:event_jbtDelActionPerformed


    private void jbtCancelActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jbtCancelActionPerformed
        DisableDeparmentInfo();
}//GEN-LAST:event_jbtCancelActionPerformed

    private void jbtSaveActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jbtSaveActionPerformed
    if(jtfName.getText().isEmpty() || jcbbLocation.getSelectedIndex() == -1){
        
        JOptionPane.showMessageDialog(this, "The Department Name and Loacation can not empty!","Warning",JOptionPane.OK_OPTION);
        return;
    }else{
      
        Department dept = new Department();
        dept.setDepartmentID(Integer.parseInt(jtfID.getText()));
        dept.setDepartmentName(jtfName.getText());
        dept.setLocationAddress(jcbbLocation.getSelectedItem().toString());
        dept.setBooleanStatus(jcbStatus.isSelected());
        
        boolean isUpdate = deptdata.updateADepartment(dept);
        if(isUpdate){
            JOptionPane.showMessageDialog(this, "Update Sucssesfully!");
            afterInit();
        }else{
            JOptionPane.showMessageDialog(this, "Update unsuccessfully");
        }
    }   
}//GEN-LAST:event_jbtSaveActionPerformed

