    @Override
public void valueChanged(ListSelectionEvent e) {
         if (!e.getValueIsAdjusting()&&checkvaluechg)
        {
  
     DefaultListSelectionModel listModel = (DefaultListSelectionModel)e.getSource();
     int selectedIndex = listModel.getMinSelectionIndex();
     tf_Allowance.setText(jTable1.getValueAt(selectedIndex,6).toString());
     tf_CreationDate.setText(jTable1.getValueAt(selectedIndex,7).toString());
     tf_DepartmentName.setText(jTable1.getValueAt(selectedIndex,11).toString());
     tf_EmployeeName.setText(jTable1.getValueAt(selectedIndex,2).toString());
     tf_LocationAddress.setText(jTable1.getValueAt(selectedIndex,8).toString());
     tf_ProjectName.setText(jTable1.getValueAt(selectedIndex,10).toString());
     tf_RecordID.setText(jTable1.getValueAt(selectedIndex,0).toString());
     tf_RelievingDate.setText(jTable1.getValueAt(selectedIndex,4).toString());
     emp = em.getAllEmployeeByID(Integer.parseInt(jTable1.getValueAt(selectedIndex,1).toString()));
     tf_ReportMan.setText(jTable1.getValueAt(selectedIndex,5).toString());
     txtReason.setText(jTable1.getValueAt(selectedIndex,9).toString());
     tf_Status.setText(jTable1.getValueAt(selectedIndex,12).toString());
     irecord=Integer.parseInt(jTable1.getValueAt(selectedIndex,0).toString());
     btnreq.setEnabled(true);
     btndel.setEnabled(true);
     
         }
    }

