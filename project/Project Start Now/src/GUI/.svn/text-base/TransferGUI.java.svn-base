    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton1ActionPerformed
        if(!tf_RecordID.getText().isEmpty()){
        if(tf_Status.getText().equalsIgnoreCase("pending")){
            JOptionPane.showMessageDialog(this, "Please appove or deny this transfer record before generate letter.");
            return;
        }

        viewletter = new ViewLetter(null, false, tf_Status.getText(), Integer.parseInt(tf_RecordID.getText()));
        viewletter.setVisible(true);
        if(tf_RecordID.getText().equalsIgnoreCase("pending")){
            JOptionPane.showMessageDialog(this, "Please appove or deny this transfer record before generate letter.");
            return;
        }
        
        }else{
           JOptionPane.showMessageDialog(this, "Please choose a transfer record before generate letter.");
        }

    }//GEN-LAST:event_jButton1ActionPerformed

