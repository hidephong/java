    @Override
    public void valueChanged(ListSelectionEvent e) {
                if (!e.getValueIsAdjusting()&&(event == 1)) {
                    DefaultListSelectionModel listModel = (DefaultListSelectionModel)e.getSource();
                    int selectedIndex = listModel.getMinSelectionIndex();
                    IDTextField.setText(projectTable.getValueAt(selectedIndex,0).toString());
                    nameTextField.setText(projectTable.getValueAt(selectedIndex,1).toString());
                    String status = projectTable.getValueAt(selectedIndex,2).toString();
                    if(status.equals("enabled"))
                    {
                        statusCombo.setSelectedIndex(0);
                    }
                    else if(status.equals("disabled"))
                    {
                        statusCombo.setSelectedIndex(1);
                    }
                }
            }

    private void editBtnActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_editBtnActionPerformed
        saveBtn.setVisible(true);
        cancelBtn.setVisible(true);
        if(!IDTextField.getText().equals(""))
        {
            nameTextField.setEditable(true);
            statusCombo.setEditable(true);
        }
        else {
            JOptionPane.showMessageDialog(this, "You must choose a project", "WARNING", JOptionPane.ERROR_MESSAGE);
        }
}//GEN-LAST:event_editBtnActionPerformed

    private void projectTableMouseReleased(java.awt.event.MouseEvent evt) {//GEN-FIRST:event_projectTableMouseReleased
    }//GEN-LAST:event_projectTableMouseReleased

    private void saveBtnActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_saveBtnActionPerformed
        if(!IDTextField.getText().equals(""))
        {
            pm.UpdateProject(nameTextField.getText(), statusCombo.getSelectedItem().toString(), Integer.parseInt(IDTextField.getText()));

            IDTextField.setText("");
            nameTextField.setText("");
            statusCombo.setSelectedIndex(2);
            IDTextField.setEditable(false);
            nameTextField.setEditable(false);
            statusCombo.setEditable(false);
            JOptionPane.showMessageDialog(this, "Save successfully", "MESSAGE", JOptionPane.INFORMATION_MESSAGE);

            event = 0;
            fillProjectTable();
            event = 1;
            saveBtn.setVisible(false);
        cancelBtn.setVisible(false);
        }
        else {
            JOptionPane.showMessageDialog(this, "You must choose a project", "WARNING", JOptionPane.ERROR_MESSAGE);
        }
    }//GEN-LAST:event_saveBtnActionPerformed

    private void cancelBtnActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_cancelBtnActionPerformed
        saveBtn.setVisible(false);
        cancelBtn.setVisible(false);
        nameTextField.setEditable(false);
        statusCombo.setEditable(false);
        IDTextField.setText("");
        nameTextField.setText("");
        statusCombo.setSelectedIndex(2);
}//GEN-LAST:event_cancelBtnActionPerformed

    
