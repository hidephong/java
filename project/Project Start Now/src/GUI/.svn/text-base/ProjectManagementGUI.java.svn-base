    private void deleteBtnActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_deleteBtnActionPerformed

            if(!IDTextField.getText().equals(""))
            {
                int projectID = Integer.parseInt(IDTextField.getText());

                int choice = JOptionPane.showConfirmDialog(this, "Do you want to delete selected project?", "Confirmation", JOptionPane.OK_CANCEL_OPTION);

                if(choice == 0)
                {
                    boolean bool = pm.DeleteAProject(Integer.parseInt(IDTextField.getText()));

                    if(bool == true)
                    {
                        nameTextField.setEditable(false);
                        statusCombo.setEditable(false);
                        IDTextField.setText("");
                        nameTextField.setText("");
                        statusCombo.setSelectedIndex(2);

                        event = 0;
                        fillProjectTable();
                        event = 1;
                    }
                    else
                    {
                        JOptionPane.showMessageDialog(this, "Can not delete because this record is in use. The status of this project will be set to disabled", "WARNING", JOptionPane.ERROR_MESSAGE);
                        
                        pm.UpdateAfterDelete(projectID);
                        
                        event = 0;
                        fillProjectTable();
                        event = 1;
                    }
                }
                else
                {
                }
            }
            else
            {
                JOptionPane.showMessageDialog(this, "You must choose a project", "WARNING", JOptionPane.ERROR_MESSAGE);
            }
    }//GEN-LAST:event_deleteBtnActionPerformed

